{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"title": "Required Fields",
	"description": "Makes 'resourceInfo' and 'metadataInfo' required properties of 'metadata'.",
	"$id": "require-fields.json",
	"version": "1.0.0",
	"properties": {
		"metadata": {
			"properties": {
				"resourceInfo": {},
				"metadataInfo": {},
				"resourceDistribution": {},
				"funding": {},
				"associatedResource": {}
			},
			"required": [
				"resourceInfo",
				"metadataInfo"
			]
		}
	}
}
